extends layout

block content
  include partials/header
  .services-container.sub-container
    .text-container
      h2 Catering Available For
      .event-list
        - 
          var list = ['Garden Parties', 'Shindigs', 'Bar-Mitzvahs', 'Funerals', 'Quincea√±eras', 'Late-Night Raves', 'Baptisms', 'Barn Dances']
        each item in list
          div(id=list.indexOf(item))
            span= item
      .contact
        a(href='/contact') Contact Us
    .carousel
      .slider
        - for(var i = 1; i < 4; i++)
          div
            img(src='/imgs/image' + i + '.jpg')

block scripts
  script(src='/services.bundle.js')
